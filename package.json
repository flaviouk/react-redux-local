    "redux-saga": "0.16.0",
    "webpack": "4.5.0",
    "webpack-cli": "2.0.14",
    "webpack-merge": "4.1.2"
  },
  "peerDependencies": {
    "react": ">=16.3.0",
    "redux": ">=3.7.2",
    "redux-saga": ">=0.16.0"
  },
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  },
  "keywords": [
    "react",
    "context",
    "redux",
    "local",
    "recompose"
